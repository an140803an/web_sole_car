<%@ page language="java" contentType="text/html; charset=utf-8"
	pageEncoding="utf-8"%>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<%@ taglib prefix="fmt" uri="http://java.sun.com/jsp/jstl/fmt"%>
<%@ taglib prefix="form" uri="http://www.springframework.org/tags/form"%>
<!DOCTYPE html>
<html lang="en">

<head>
<title>Trang</title>
<meta charset="utf-8">
<meta name="viewport" content="width=device-width, initial-scale=1">
<link
	href="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css"
	rel="stylesheet">
<script
	src="https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/js/bootstrap.bundle.min.js"></script>
<link rel="stylesheet"
	href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.3/font/bootstrap-icons.css">

<script
	src="https://connect.facebook.net/en_US/sdk.js#xfbml=1&amp;version=v2.9"></script>
<script
	src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>

<script
	src="https://ajax.googleapis.com/ajax/libs/angularjs/1.8.2/angular.min.js"></script>
<link rel="stylesheet" href="index.css">
<link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap-icons@1.10.5/font/bootstrap-icons.css">
</head>
<style>
body {
	background: white;
	color: black;
	font-family: 'Source Sans Pro', sans-serif;
	text-rendering: optimizeLegibility;
}

.container {
	margin-top: 50px;
}

/* filter color */
.leftbar ul {
	padding-left: 0;
}

.leftbar ul li {
	list-style: none;
}

.leftbar ul li a {
	text-decoration: none;
	color: #888888;
}

.leftbar ul li a.active {
	color: black;
}

.leftbar .filter-color ul {
	display: flex;
}

.leftbar .filter-color ul li {
	margin-right: 5px;
}

.sort-filter {
	display: flex;
	margin-bottom: 20px;
}

.color-filter1 {
	background-color: #00bbec;
}

.color-filter2 {
	background-color: #2c6ed5;
}

.color-filter3 {
	background-color: #ffa037;
}

.color-filter4 {
	background-color: #ff5337;
}

.color-filter5 {
	background-color: #a88c77;
}

.color-filter6 {
	background-color: #393939;
}

.color-filter7 {
	background-color: #cccccc;
}

.checkbox-color-filter {
	display: none;
}

.color-filter {
	display: block;
	width: 25px;
	height: 25px;
	cursor: pointer;
	border-radius: 50%;
}

.checkbox-color-filter:checked+.color-filter {
	box-shadow: 0 0 0px 2px black;
	-moz-box-shadow: 0 0 0px 2px black;
	-webkit-box-shadow: 0 0 0px 2px black;
	-o-box-shadow: 0 0 0px 2px black;
	-ms-box-shadow: 0 0 0px 2px black;
}

a.btn {
	border-radius: 4px;
	box-shadow: 0 2px 0px 0 rgba(0, 0, 0, 0.25);
	display: inline-block;
	padding: 6px 30px 8px;
	position: relative;
	text-decoration: none;
}

/* select product */
:root {
	--background-gradient: linear-gradient(30deg, #f39c12 30%, #f1c40f);
	--gray: #34495e;
	--darkgray: #2c3e50;
}

select {
	appearance: none;
	outline: 0;
	border: 0;
	box-shadow: none;
	flex: 1;
	padding: 0 1em;
	color: #fff;
	background-color: var(--darkgray);
	background-image: none;
	cursor: pointer;
}

select::-ms-expand {
	display: none;
}

.select {
	position: relative;
	display: flex;
	width: 20em;
	height: 3em;
	border-radius: .25em;
	overflow: hidden;
	margin-right: 30px;
}

.select::after {
	content: '\25BC';
	position: absolute;
	top: 0;
	right: 0;
	padding: 1em;
	background-color: #34495e;
	transition: .25s all ease;
	pointer-events: none;
}

.select:hover::after {
	color: #f39c12;
}

/* logo car */
.logo-car{
	border: none;
	background-color: white;
	width: 250px; 
}

.styled-radio {
	margin-top: 15px;
	margin-bottom: 30px;
	width: 150px;
	height: 100px;
	position: relative;
}

.invisible-radio {
	/* per https://a11yproject.com/posts/how-to-hide-content/ */
	position: absolute;
	height: 1px;
	width: 1px;
	overflow: hidden;
	clip: rect(1px, 1px, 1px, 1px);
	/* IE6, IE7 */
	clip: rect(1px, 1px, 1px, 1px);
}

.invisible-radio+label .styled-radio::after {
	content: attr(data-text);
	font-size: 1.5em;
	display: inline-block;
	box-sizing: border-box;
	padding: 20px;
	width: 150px;
	text-align: center;
	position: absolute;
	bottom: -60px;
}

.invisible-radio+label .styled-radio {
	background-position: center center;
	background-size: 80% auto;
	background-repeat: no-repeat;
}

#radio1.invisible-radio+label .styled-radio {
	background-image:
		url(data:image/png;base64,);
}

#radio2.invisible-radio+label .styled-radio {
	background-image:
		url(data:image/png;base64,);
}

#radio3.invisible-radio+label .styled-radio {
	background-image:
		url(data:image/png;base64,);
}

#radio4.invisible-radio+label .styled-radio {
	background-image:
		url(data:image/jpeg;base64,);
}

#radio5.invisible-radio+label .styled-radio {
	background-image:
		url(data:image/png;base64,);
}

.invisible-radio:checked+label .styled-radio {
	background-color: white;
}

.invisible-radio:not(:checked)+label:hover .styled-radio {
	opacity: 0.7;
}

.invisible-radio:focus+label .styled-radio {
	transform: scale(1, 1);
	animation: pulsate 1s alternate ease-in-out infinite;
}

@
keyframes pulsate {from { transform:scale(1, 1);
	box-shadow: none;
}

to {
	transform: scale(1.05, 1.05);
	box-shadow: 1px 4px 8px 1px rgba(0, 0, 0, 0.2);
}

}

/* cards product */
.card-img-overplay {
	color: red;
}

img {
	height: 180px;
	width: 100%;
}

div.cards {
	margin: 30px auto;
	max-width: 960px;
	text-align: center;
}

div.card {
	display: inline-block;
	margin: 8px;
	max-width: 300px;
	perspective: 1000;
	position: relative;
	text-align: left;
	width: 300px;
}

/* Pagination */
.two {
	width: 15.33%;
}

.pagination {
	padding: 30px 0;
}

.pagination ul {
	margin: 0;
	padding: 0;
	list-style-type: none;
}

.pagination a {
	display: inline-block;
	padding: 10px 18px;
	color: #222;
	text-decoration: none;
	font-size: 14px;
}

.p2 .is-active li {
	font-weight: bold;
	border-bottom: 3px solid #2ecc71;
}

/* text */
span {
	display: block;
	height: 2px;
	position: absolute;
	top: 16px;
	width: 12px;
}

h2 {
	font-size: 20px;
	font-weight: 700;
	letter-spacing: -0.05em;
	margin: 0;
	padding: 0;
}
</style>

<body>
	<main>
<%@include file="header.jsp" %>
		<!-- Content page -->

		<div class="container">
			<form action="/product/page" method="post">
			<div class="row">
				<div class="col-sm-6 col-md-4 col-lg-3">
					<div class="leftbar">
						<h4 class="">Chọn lọc</h4>

						<div class="filter-price">
							<div class="">Hãng xe:</div>
							<div>
								<button class="logo-car" type="submit" name="keywords" value="Toyota">
									<input type="radio" id="radio1" class="invisible-radio"> 
									<label for="radio1">
										<div class="styled-radio white"></div>
									</label> 
								</button>
								<button class="logo-car" type="submit" name="keywords" value="Mercedes">
									<input type="radio" name="radios" id="radio2" class="invisible-radio"> 
									<label for="radio2">
										<div class="styled-radio white"></div>
									</label> 
								</button>
								<button class="logo-car" type="submit" name="keywords" value="Ford">
									<input type="radio" name="radios" id="radio3" class="invisible-radio"> 
									<label for="radio3">
										<div class="styled-radio white"></div>
									</label>
								</button>
								<button class="logo-car" type="submit" name="keywords" value="BMW">
									<input type="radio" name="radios" id="radio4" class="invisible-radio"> 
									<label for="radio4">
										<div class="styled-radio white"></div>
									</label> 
								</button> 
								<button class="logo-car" type="submit" name="keywords" value="Kia">
									<input type="radio" name="radios" id="radio5" class="invisible-radio"> 
									<label for="radio5">
										<div class="styled-radio white"></div>
									</label>
								</button>
								
							</div>
						</div>

						<div class="filter-color">
							<div class="">Color:</div>

							<ul class="">
								<li class=""><input class="checkbox-color-filter"
									id="color-filter1" type="checkbox" name="color-filter1">
									<label class="color-filter color-filter1" for="color-filter1"></label>
								</li>

								<li class="m-r-10"><input class="checkbox-color-filter"
									id="color-filter2" type="checkbox" name="color-filter2">
									<label class="color-filter color-filter2" for="color-filter2"></label>
								</li>

								<li class="m-r-10"><input class="checkbox-color-filter"
									id="color-filter3" type="checkbox" name="color-filter3">
									<label class="color-filter color-filter3" for="color-filter3"></label>
								</li>

								<li class="m-r-10"><input class="checkbox-color-filter"
									id="color-filter4" type="checkbox" name="color-filter4">
									<label class="color-filter color-filter4" for="color-filter4"></label>
								</li>

								<li class="m-r-10"><input class="checkbox-color-filter"
									id="color-filter5" type="checkbox" name="color-filter5">
									<label class="color-filter color-filter5" for="color-filter5"></label>
								</li>

								<li class="m-r-10"><input class="checkbox-color-filter"
									id="color-filter6" type="checkbox" name="color-filter6">
									<label class="color-filter color-filter6" for="color-filter6"></label>
								</li>

								<li class="m-r-10"><input class="checkbox-color-filter"
									id="color-filter7" type="checkbox" name="color-filter7">
									<label class="color-filter color-filter7" for="color-filter7"></label>
								</li>
							</ul>
						</div>
					</div>
				</div>

				<div class="col-sm-6 col-md-8 col-lg-9">
					<H1>
						Tất cả sản phẩm
					</H1>
					<hr>
					<!--  -->
					<div class="flex-sb-m flex-w p-b-35">
						<div class="sort-filter">
							<div class="select">
								<select onchange="window.location=this.value">
									<option value="/product/page">Mặc định</option>
									<option value="/product/sortnamelow">A ---> Z</option>
									<option value="/product/sortnamehigh">Z ---> A</option>
									<option value="/product/sortpricelow">Giá:
										thấp đến cao</option>
									<option value="/product/sortpricehigh">Giá: cao đến thấp</option>
								</select>
							</div>

							<div class="select">
								<select onchange="window.location=this.value">
									<option value="1">Giá</option>
									<option value="/product/sortprice57">500.000.000 - 700.000.000</option>
									<option value="/product/sortprice710">700.000.000 - 1.000.000.000</option>
									<option value="/product/sortprice1050">1.000.000.000 - 5.000.000.000</option>
									<option value="/product/sortpricehigh50">5.000.000.000+</option>
								</select>
							</div>
						</div>
					</div>

					<div class="cards">
						<c:forEach items="${page.content}" var="product">
							<div class="card">
								<div class="card-image-holder">
									<img class="card-image" src="${product.image}" alt="Card image" />
									<div class="card-img-overplay">${product.price}đ</div>
								</div>
								<div class="card-title">
									<h2>${product.productName}</h2>
									<hr>
									<div class="btn-group">
										<a class="btn btn-details" style="margin-right: 18px"
											href="/detail?id=${product.productId}"><svg
												xmlns="http://www.w3.org/2000/svg" width="20" height="24"
												viewBox="0 0 24 24" fill="none" stroke="currentColor"
												stroke-width="2" stroke-linecap="round"
												stroke-linejoin="round"
												class="feather feather-shopping-cart">
						<circle cx="9" cy="21" r="1"></circle>
						<circle cx="20" cy="21" r="1"></circle>
						<path
													d="M1 1h4l2.68 13.39a2 2 0 0 0 2 1.61h9.72a2 2 0 0 0 2-1.61L23 6H6"></path>
					</svg> Mua hàng</a> <a class="btn btn-success" href="#"><svg
												xmlns="http://www.w3.org/2000/svg" width="20" height="24"
												viewBox="0 0 24 24" fill="none" stroke="currentColor"
												stroke-width="2" stroke-linecap="round"
												stroke-linejoin="round" class="feather feather-eye">
						<path d="M1 12s4-8 11-8 11 8 11 8-4 8-11 8-11-8-11-8z"></path>
						<circle cx="12" cy="12" r="3"></circle>
					</svg> Details</a>
									</div>
								</div>
							</div>
						</c:forEach>
					</div>

					<!-- <div class="pagination p2">
						<ul>
							<a href="#">
								<li><b>Previous</b></li>
							</a>
							<a href="#">
								<li>1</li>
							</a>
							<a class="is-active" href="#">
								<li>2</li>
							</a>
							<a href="#">
								<li>3</li>
							</a>
							<a href="#">
								<li>4</li>
							</a>
							<a href="#">
								<li>5</li>
							</a>
							<a href="#">
								<li>6</li>
							</a>
							<a href="#">
								<li><b>Next</b></li>
							</a>
						</ul>
					</div> -->

					<nav aria-label="Page navigation example">
						<ul class="pagination justify-content-center">
							<c:if test="${page.number == 0}">
								<li class="page-item disabled"><a href="/product/page?product=0"
									class="page-link"><i class="bi bi-chevron-double-left"></i></a>
								</li>
								<li class="page-item disabled"><a
									href="/product/page?product=${page.number-1}" class="page-link"
									href="#"><i class="bi bi-chevron-left"></i></a></li>
							</c:if>
							<c:if test="${page.number != 0}">
								<li class="page-item"><a href="/product/page?product=0"
									class="page-link"><i class="bi bi-chevron-double-left"></i></a>
								</li>
								<li class="page-item"><a
									href="/product/page?product=${page.number-1}" class="page-link"
									href="#"><i class="bi bi-chevron-left"></i></a></li>
							</c:if>
							<c:if test="${page.number == page.totalPages-1}">
								<li class="page-item disabled"><a
									href="/product/page?product=${page.number+1}" class="page-link"
									href="#"><i class="bi bi-chevron-right"></i></a></li>
								<li class="page-item disabled"><a
									href="/product/page?product=${page.totalPages-1}" class="page-link"
									href="#"><i class="bi bi-chevron-double-right"></i></a></li>
							</c:if>
							<c:if test="${page.number != page.totalPages-1}">
								<li class="page-item"><a
									href="/product/page?product=${page.number+1}" class="page-link"
									href="#"><i class="bi bi-chevron-right"></i></a></li>
								<li class="page-item"><a
									href="/product/page?product=${page.totalPages-1}" class="page-link"
									href="#"><i class="bi bi-chevron-double-right"></i></a></li>
							</c:if>
						</ul>
					</nav>
				</div>

			</div>
		</form>
		</div>
		<%@include file="footer.jsp" %>


		


			
	</main>
</body>

</html>